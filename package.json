{
  "scripts": {
    "dev": "yarn tsx --watch & yarn nodemon --watch dist dist/index.js",
    "start": "yarn build && yarn serve",
    "build": "yarn tsc",
    "serve": "NODE_OPTIONS=--openssl-legacy-provider node dist/index.js",
    "test": "yarn lint && yarn typecheck && yarn check-format",
    "check-format": "prettier --list-different '**/*.ts'",
    "format": "prettier --write --list-different '**/*.ts'",
    "lint": "eslint '**/*.ts'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "0.3.0",
    "@oraichain/cosmosjs": "^0.0.84",
    "discord-api-types": "0.27.2",
    "discord.js": "13.6.0",
    "dotenv": "^16.0.0"
  },
  "name": "likecoin-faucet",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "4.17.13",
    "eslint": "^8.9.0",
    "prettier": "^2.5.1",
    "typescript": "4.5.5"
  }
}
